<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane fx:id="rootPane" prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="it.polimi.ingsw.GUI.StartMenuController">
    <children>
        <VBox alignment="CENTER" style="-fx-background-color: #999919;" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                <Button fx:id="createButton" mnemonicParsing="false"  prefHeight="75.0" prefWidth="680.0" text="CREATE MATCH" onAction="#handleCreate">
                    <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                    </VBox.margin>
                </Button>
                <Button fx:id="joinButton" mnemonicParsing="false" prefHeight="75.0" prefWidth="680.0" text="JOIN MATCH" onAction="#handleJoin">
                    <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="40.0" />
                    </VBox.margin>
                </Button>
            </children></VBox>
    </children>
</AnchorPane>
